{
  "hosting": {
    "redirects": [
      {
        "source": "/config.json",
        "destination": "/reference/airnode/latest/deployment-files/config-json.html",
        "type": 301
      },
      {
        "source": "/latest",
        "destination": "/reference/airnode/latest/",
        "type": 301
      },
      {
        "source": "/next",
        "destination": "/reference/airnode/next/",
        "type": 301
      },
      {
        "source": "/airnode",
        "destination": "/reference/airnode/latest/",
        "type": 301
      },
      {
        "source": "/ois",
        "destination": "/reference/ois/latest/",
        "type": 301
      },
      {
        "source": "/dapis",
        "destination": "/reference/dapis/understand/",
        "type": 301
      },
      {
        "source": "/qrng",
        "destination": "/reference/qrng/",
        "type": 301
      },
      {
        "source": "/dao-members",
        "destination": "/reference/dao-members/",
        "type": 301
      }
    ],
    "public": "docs/.vitepress/dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
